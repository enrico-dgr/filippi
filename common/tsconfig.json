{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"c-assets/*": ["assets/*"],
			"c-builders/*": ["builders/*"],
			"c-entities/*": ["entities/*"],
			"c-systems/*": ["systems/*"],
			"c-types/*": ["types/*"],
		},
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	}
}
